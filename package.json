    "@types/node-cron": "^3.0.11",
    "axios": "^1.7.9",
    "bcrypt": "^5.1.1",
    "body-parser": "^1.20.3",
    "cookie-parser": "~1.4.4",
    "cors": "^2.8.5",
    "date-fns": "^4.1.0",
    "debug": "~2.6.9",
    "express": "^4.21.1",
    "hashids": "^2.3.0",
    "http-errors": "~1.6.3",
    "i18next": "^24.2.0",
    "i18next-fs-backend": "^2.6.0",
