    "kysely": "^0.27.4",
    "module-alias": "^2.2.3",
    "moment": "^2.30.1",
    "morgan": "~1.9.1",
    "multer": "^1.4.5-lts.1",
    "nanoid": "^5.0.8",
    "node-cron": "^4.2.1",
    "nodemon": "^3.1.7",
    "reflect-metadata": "^0.2.2",
