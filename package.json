    "build": "tsc",
    "format": "prettier --write .",
    "lint": "eslint . --ext .ts",
    "swagger-autogen": "ts-node app/swagger/generate-swagger-doc.ts",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:ci": "jest --ci --coverage --watchAll=false"
  },
