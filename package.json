{
  "name": "air-quality",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "node dist/app/server.js",
    "dev": "ts-node-dev --respawn --transpile-only -r dotenv/config -r tsconfig-paths/register app/server.ts",
